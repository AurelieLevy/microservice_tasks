doctype 
html(lang="fr", dir="ltr")
	head
		title Frontend
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
		meta(http-equiv="Content-Type", content="text/html")
		meta(name="description", content="")
		meta(name="keywords", content="")
		meta(name="viewport", content="width=device-width, initial-scale=1, user-scalable=0, maximum-scale=1, minimum-scale=1, minimal-ui")
		
		link(rel="stylesheet", href="/css/font.css" , media="screen")
		link(rel="stylesheet", href="/css/app.css?" + buildVersion, media="screen")

	body(ontouchstart="")
		div(id="react-app")
		
		script(type="text/javascript").
			if(/MSIE \d|Trident.*rv:/.test(navigator.userAgent))
			document.write('<script type="text/javascript" src="/js/polyfill.min.js"><\/script>');

		script(type="text/javascript" src="/js/modernizr.js")
		script(type="text/javascript" src="/js/app.bundle.js?" + buildVersion)